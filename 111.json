            "type": "string"
          },
          "userId": {
            "type": "string"
          },
          "status": {
            "type": "string",
            "example": "filled",
            "enum": [
              "filled",
              "rejected"
            ]
          },
          "symbol": {
            "type": "string",
            "example": "BTC/USD.E"
          },
          "amount": {
            "type": "number",
            "format": "double",
            "example": 0.01
          },
          "rate": {
            "type": "number",
            "format": "double",
            "example": 4200
          },
          "side": {
            "type": "string",
            "enum": [
              "buy",
              "sell"
            ]
          },
          "created": {
            "type": "integer",
            "format": "int64"
          },
          "executed": {
            "type": "integer",
            "format": "int64"
          },
          "commission": {
            "type": "number",
            "format": "double",
            "example": 0.000025
