          "lastTime": {
            "type": "integer",
            "format": "int64"
          }
        }
      },
      "Wallet": {
        "type": "object",
        "properties": {
          "currency": {
            "type": "string",
            "example": "BTC"
          },
          "balance": {
            "type": "number",
            "format": "double",
            "example": 1.5
          },
          "hold": {
            "type": "number",
            "format": "double",
            "example": 0.5
          }
        }
      },
      "Market": {
