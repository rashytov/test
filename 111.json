          "symbol": {
            "type": "string",
            "example": "BTC/USD.E"
          },
          "amount": {
            "type": "number",
            "format": "double",
            "example": 0.01
          },
          "rate": {
            "type": "number",
            "format": "double",
            "example": 4200
